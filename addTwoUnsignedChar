#include<stdio.h>
main()
{
        unsigned char ch0,ch1,ch2,ch3;

        ch1=2;
        ch2=255;

        printf("ch1=%u ch2=%u \n",ch1,ch2);

        ch3=ch1+ch2;
        if(ch3<ch1)
                printf("Result is going out of scope\n");
        else
                printf("%u + %u = %u\n",ch1,ch2,ch3);
}
